{
    "name": "crysalead/storage-stream",
    "description": "Object-Oriented API for PHP streams",
    "keywords": ["Stream"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "crysalead/kahlan": "~2.0",
        "crysalead/dir": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Storage\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Storage\\Stream\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
