{
    "name": "crysalead/storage-stream",
    "description": "Object-Oriented API for PHP streams",
    "keywords": ["Stream"],
    "license": "MIT",
    "authors": [
        {
            "name": "CrysaLEAD"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-fileinfo": "*",
        "psr/http-message": "~2.0"
    },
    "require-dev": {
        "kahlan/kahlan": "~6.0",
        "crysalead/dir": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Storage\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Storage\\Stream\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
